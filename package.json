{
  "name": "@dataxpdtn/mui-json-viewer",
  "description": "Interactive Json Viewer, but not only a json viewer",
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "version": "5.0.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dataxpdtn/mui-json-viewer.git"
  },
  "author": "github@dataexpedition.net",
  "bugs": "https://github.com/dataxpdtn/mui-json-viewer/issues",
  "homepage": "https://github.com/dataxpdtn/mui-json-viewer#readme",
  "keywords": [
    "react-18",
    "react",
    "react-json",
    "react-json-viewer",
    "array-viewer",
    "component",
    "interactive",
    "interactive-json",
    "json",
    "json-component",
    "json-display",
    "json-tree",
    "json-view",
    "json-viewer",
    "json-inspector",
    "json-tree",
    "tree",
    "tree-view",
    "treeview"
  ],
  "types": "dist/index.d.ts",
  "jsdelivr": "dist/browser.js",
  "unpkg": "dist/browser.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm --filter \"@dataxpdtn/mui-json-viewer-docs\" run dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepack": "pinst --disable",
    "prepare": "husky",
    "postpack": "pinst --enable",
    "lint": "eslint --fix",
    "lint:ci": "eslint --cache --max-warnings 0",
    "build": "tsc && rollup -c rollup.config.ts --configPlugin swc3"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "zustand": "^5.0.9"
  },
  "lint-staged": {
    "!*.{ts,tsx,js,jsx}": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "npx eslint --cache --fix"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/material": "^7",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.39.1",
    "@mui/material": "^7.3.5",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@swc/core": "^1.15.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "expect-type": "^1.2.2",
    "globals": "^16.5.0",
    "husky": "9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "pinst": "^3.0.0",
    "prettier": "^3.7.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-swc3": "^0.12.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.6",
    "vitest": "^4.0.14"
  },
  "overrides": {
    "browserslist": "4.23.3",
    "caniuse-lite": "1.0.30001660"
  }
}
